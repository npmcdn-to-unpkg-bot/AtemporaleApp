section#work.section
    .section-row-container
        .section-row
            .container
                .row
                    .col-lg-3
                        h1.section-title.show-counter
                            !='Nosso Port fólio'
                            small
                                !='Conheça nossos projetos recentes e se encante!'

                    .col-lg-8.col-lg-push-1
                        .row
